<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<settings
     cacheModelsEnabled="true"
     enhancementEnabled="true"
     lazyLoadingEnabled="true"
     maxRequests="32"
     maxSessions="10"
     maxTransactions="5"
     useStatementNamespaces="true"
    />
	<sqlMap resource="ARTICLE_SqlMap.xml" />
	<sqlMap resource="ADMIN_SqlMap.xml" />
	<sqlMap resource="SLIDE_SqlMap.xml" />
	<sqlMap resource="CATEGORY_SqlMap.xml" />
	<sqlMap resource="UPLOAD_SqlMap.xml" />
	<sqlMap resource="PUBLICATION_SqlMap.xml" />
	<sqlMap resource="PARAMETER_SqlMap.xml" />
	<sqlMap resource="LINK_SqlMap.xml" />
	
	
	<sqlMap resource="ACTIVITY_SqlMap.xml" />
	<sqlMap resource="ACTIVITY_APPLICATION_SqlMap.xml" />
	<sqlMap resource="ACTIVITY_NEWS_SqlMap.xml" />
	<sqlMap resource="ACTIVITY_PHOTO_SqlMap.xml" />
	<sqlMap resource="ACTIVITY_VIDEO_SqlMap.xml" />
	<sqlMap resource="COMMENT_SqlMap.xml" />
	<sqlMap resource="DESIGNER_SqlMap.xml" />
	<sqlMap resource="WORK_SqlMap.xml" />
	
	<sqlMap resource="REPORTER_SqlMap.xml" />
	<sqlMap resource="BLOG_SqlMap.xml" />
</sqlMapConfig>